	"eslintConfig": {
		"extends": "airbnb-base",
		"env": {
			"node": true,
			"es6": true,
			"jest": true
		},
		"rules": {
			"func-names": "off",
			"indent": [
				"error",
				"tab"
			],
			"import/no-unresolved": "off",
			"no-plusplus": [
				"error",
				{
					"allowForLoopAfterthoughts": true
				}
			],
			"no-tabs": [
				"error",
				{
					"allowIndentationTabs": true
				}
			],
			"padded-blocks": [
				"error",
				{
					"classes": "always"
				}
			],
			"space-in-parens": [
				"error",
				"always"
			],
			"template-curly-spacing": [
				"error",
				"always"
			]
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint --silent"
		}
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"scope": "@saritvakrat"
	},
	"directories": {
		"doc": "docs",
		"test": "tests"
	},
